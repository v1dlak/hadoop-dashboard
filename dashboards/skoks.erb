    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="twitter_mentions_szn" data-view="Comments" style="background-color:#96bf48;" data-moreinfo="Tweets @seznam_cz"></div>
      <i class="icon-twitter icon-background"></i>
